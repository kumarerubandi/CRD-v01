               "type":"open-choice",
               "required":false,
               "repeats":true,
               "option":[
                  {
                     "valueCoding":{
                        "code":"CVA with sequelae (late effect of CVA) that impair mobility and result in be confinement"
                     }
                  },
                  {
                     "valueCoding":{
                        "code":"DVT requires elevation of lower extremity"
                     }
                  }
               ]
            },
