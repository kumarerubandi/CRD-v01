               "type":"open-choice",
               "required":true,
               "repeats":true,
               "option":[
                  {
                     "valueCoding":{
                        "code":"Dialysis"
                     }
                  },
                  {
                     "valueCoding":{
                        "code":"Wound care"
                     }
                  },
                  {
                     "valueCoding":{
                        "code":"Radiation therapy"
                     }
