         "type":"group",
         "item":[
            {
               "linkId":"8.1",
               "text":"Physician Certification Statement",
               "type":"attachment",
               "required":true
            },
            {
               "linkId":"8.2",
               "text":"Documentation from the medical record to support the medical necessity of repetitive, scheduled non-emergent ambulance transport",
               "type":"attachment",
               "required":true
            },
            {
               "linkId":"8.3",
               "text":"Information on the origin and destination of the transports",
               "type":"attachment",
               "required":true
            },
            {
               "linkId":"8.4",
               "text":"Any other relevant document as deemed necessary by the MAC to process the prior authorization",
               "type":"attachment",
               "required":false
            }
