                  {
                     "valueCoding":{
                        "code":"Unable to maintain erect sitting position in a chair for time needed to transport, due to moderate muscular weakness and de-conditioning"
                     }
                  },
                  {
                     "valueCoding":{
                        "code":"Risk of falling off wheelchair or stretcher while in motion (not related to obesity)"
                     }
                  }
               ]
            },
            {
               "linkId":"7.4",
               "text":"Musculoskeletal",
               "type":"open-choice",
               "required":false,
               "repeats":true,
               "option":[
                  {
                     "valueCoding":{
                        "code":"Non-healed fractures requiring ambulance"
                     }
                  },
                  {
                     "valueCoding":{
                        "code":"Contractures that impair mobility and result in bed confinement"
                     }
                  },
                  {
                     "valueCoding":{
                        "code":"Incapacitating Osteoarthritis"
                     }
                  },
                  {
                     "valueCoding":{
                        "code":"Severe muscular weakness and de-conditioned state precludes any significant physical activity"
                     }
                  },
                  {
                     "valueCoding":{
                        "code":"Orthopedic device required in transit"
                     }
                  },
                  {
                     "valueCoding":{
                        "code":"Amputation(s)"
                     }
