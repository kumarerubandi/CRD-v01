      {
         "linkId":"6",
         "text":"Coverage Requirements",
         "type":"group",
         "item":[
            {
               "linkId":"6.1",
               "text":"Is the beneficiary suffering from a medical condition such that other means of transportation are contraindicated? If yes, a documented proof needs to be attached",
               "type":"boolean",
               "required":true
            },
            {
               "linkId":"6.2",
               "text":"Is the beneficiary bed-confined?",
               "type":"boolean",
               "required":true
            },
            {
               "linkId":"6.3",
               "text":"Is the beneficiary\A1\AFs medical condition, regardless of bed confinement, is such that transportation by ambulance is medically required?",
               "type":"boolean",
               "required":true
            },
            {
               "linkId":"6.4",
               "text":"Is the beneficiary unable to get up from bed without assistance?",
               "type":"boolean",
               "required":true
            },
            {
               "linkId":"6.5",
               "text":"Id the beneficiary unable to ambulate?",
               "type":"boolean",
               "required":true
            },
            {
               "linkId":"6.6",
               "text":"Is the beneficiary unable to sit in a chair or wheelchair?",
               "type":"boolean",
               "required":true
            },
            {
               "linkId":"6.7",
               "text":"How many times during a 10-day period is the ambulance service medically necessary for the beneficiary?",
               "type":"integer",
               "required":true
