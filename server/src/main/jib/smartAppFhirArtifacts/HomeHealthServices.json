      {
         "linkId":"6",
         "text":"Coverage Requirements",
         "type":"group",
         "item":[
            {
               "linkId":"6.1",
               "text":"Does there exist a normal inability to leave home in the beneficiary?",
               "type":"boolean",
               "required":true
            },
            {
               "linkId":"6.2",
               "text":"Does leaving home require a considerable and taxing effort?",
               "type":"boolean",
               "required":true
            },
            {
               "linkId":"6.3",
               "text":"Because of illness or injury, does the beneficiary need the aid of supportive devices such as crutches, canes, wheelchairs and walkers; or the use of special transportation; or the assistance of another person in order to leave their place of residence?",
               "type":"boolean",
               "required":true
            },
            {
               "linkId":"6.4",
               "text":"Is the beneficiary suffering from a condition such that leaving his or her home is medically contraindicated?",
               "type":"boolean",
               "required":true
            },
            {
               "linkId":"6.5",
               "text":"Is the beneficiary under the care of a physician?",
               "type":"boolean",
               "required":true
            },
            {
               "linkId":"6.6",
               "text":"Is the beneficiary receiving services under a plan of care established and periodically reviewed by a physician?",
               "type":"boolean",
               "required":true
            },
            {
               "linkId":"6.7",
               "text":"Is the beneficiary in need of skilled services",
               "type":"boolean",
               "required":true
            },
            {
               "linkId":"6.8",
               "text":"Did the beneficiary have a face-to-face encounter with a medical provider?",
               "type":"boolean",
               "required":true
            },
            {
               "linkId":"6.9",
               "text":"Date of face-to-face encounter",
               "type":"date",
               "enableWhen":[
                  {
                     "question":"6.8",
                     "answerBoolean":true
                  }
               ],
               "extension":[
                  {
                     "url":"http://hl7.org/fhir/StructureDefinition/cqif-calculatedValue",
                     "valueString":"encounterdate"
                  }
               ]
            },
            {
               "linkId":"6.10",
               "text":"Is the encounter the primary reason the patient requires home health services?",
               "type":"boolean",
               "enableWhen":[
                  {
                     "question":"6.8",
                     "answerBoolean":true
                  }
               ],
               "required":true
            },
            {
               "linkId":"6.11",
               "text":"Did an approved provider type perform the encounter?",
               "type":"boolean",
               "required":true
            }
         ]
      },
      {
